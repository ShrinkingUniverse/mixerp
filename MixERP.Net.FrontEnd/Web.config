        <add tagPrefix="mixerp" tagName="WorkflowWidget" src="~/UserControls/Widgets/WorkflowWidget.ascx"/>
        <add tagPrefix="AjaxCTK" namespace="AjaxControlToolkit" assembly="AjaxControlToolkit"/>
      </controls>
    </pages>
    <webServices>
      <protocols>
        <add name="HttpGet"/>
        <add name="HttpPost"/>
      </protocols>
    </webServices>
